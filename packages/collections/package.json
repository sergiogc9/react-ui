{
	"name": "@sergiogc9/react-ui-collections",
	"version": "0.7.1",
	"description": "React UI Kit Collections",
	"repository": "git://github.com/sergiogc9/react-ui.git",
	"license": "UNLICENSED",
	"types": "dist/types/index.d.ts",
	"keywords": [
		"ui",
		"react",
		"styled-components",
		"design-system"
	],
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist"
	],
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
