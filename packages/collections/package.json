	"scripts": {
		"build": "yarn js-bundle build --tsc-incremental",
		"watch": "yarn build --watch"
	},
	"devDependencies": {
		"@types/react": "17.0.0",
		"@types/react-dom": "17.0.0",
		"@types/styled-system": "5.1.10",
		"@types/styled-system__css": "5.0.15"
	},
	"dependencies": {
		"@sergiogc9/react-hooks": "^0.2.2",
		"@sergiogc9/react-ui": "^0.7.1",
		"@sergiogc9/react-ui-theme": "^0.7.1",
		"@sergiogc9/react-utils": "^0.0.7"
	},
	"peerDependencies": {
		"react": "17.0.1",
		"react-dom": "17.0.1",
		"styled-components": "5.2.1"
	},
	"engines": {
		"node": ">=14.x.x",
		"npm": ">=6.0.0"
	}
}
